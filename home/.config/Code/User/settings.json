{
    "git.autofetch": false,
    "editor.scrollBeyondLastLine": false,
    "editor.autoClosingBrackets": false,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.tabCompletion": true,
    "editor.detectIndentation": true,
    "editor.renderIndentGuides": true,
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.cursorBlinking": "smooth",
    "editor.cursorStyle": "line",
    "window.zoomLevel": 0,
    "window.openFoldersInNewWindow": "on",
    "window.openFilesInNewWindow": "off",
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontFamily": "menlo",
    "terminal.integrated.scrollback": 10000,
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "vs-seti",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.formatting.provider": "autopep8",
    "python.pythonPath": "/usr/local/bin/python3",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.meta": true,
        "**/*.pyc": true,
        "**/__pycache__/": true
    },
    "background.useFront": false,
    "background.useDefault": false,
    "background.customImages": [
        "/sers/murata/icloud/pics/background/chama.jpg",
        "/sers/murata/icloud/pics/background/chama.jpg",
        "/sers/murata/icloud/pics/background/chama.jpg"
    ],
    "todohighlight.keywords": [
        "WARN:",
        "NOTE:"
    ],
    // "code-runner.executorMap": {
    //     "cpp": "vscode_cpp_run(){ clang++ -std=c++14 -o out $1; if [ -f test.txt ]; then cat test.txt | ./out ;else ./out ; fi ;rm ./out; }; vscode_cpp_run ",
    //     "python": "python3 ",
    //     "d": "dub test -q ;echo "
    // },
    "latex-workshop.chktex.enabled": false,
    "latex-workshop.latex.toolchain": [
        {
            "command": "ptex2pdf",
            "args": [
                "-l",
                "-ot",
                "-kanji=utf8 -synctex=1",
                "%DOC%"
            ]
        }
    ],
    //"latexCompile.compiler": "ptex2pdf -l -ot \"-synctex=1 -file-line-error\"",
    "clang.executable": "clang++",
    "clang.cxxflags": [
        "-std=c++14"
    ],
    "clang-format.style": "Google",
    "[nim]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "terminal.integrated.lineHeight": 1,
    "editor.wordWrap": "off",
}