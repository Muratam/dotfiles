[merge]
    ff = false
[color]
    ui = auto
[alias]
    a = add -A
    b = branch
    c = commit -m
    d = diff --color-words --ignore-blank-lines -w
    l = log
    p = push
    pl = pull
    s = status --short
    po = push origin
    pom = push origin master
    plo = pull origin
    plom = pull origin master
    lst = log --stat
    co = checkout
    cob = checkout -b
    com = checkout master
    resetfile = checkout HEAD --
    ac = commit -am
    au = add -u # 新規ファイル以外 add
    ca = commit --amend -m #直前のコミットに修正を追加
    cp = cherry-pick #特定のコミットIDのもののみ反映
    dc = diff --cached #commitで反映される変更を表示
    br = browse-remote
    #pp = pull --prune
    #sta = stash
    #ap = add -p
[core]
    pager = less
    autocrlf = input
    excludesfile = ~/.gitignore
